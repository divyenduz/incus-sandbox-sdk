{
  "name": "incus-sandbox-sdk",
  "version": "0.1.0",
  "description": "A typesafe TypeScript SDK for Incus containers and VMs",
  "module": "src/index.ts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "bin": {
    "isb": "./dist/cli/bin.js"
  },
  "scripts": {
    "test": "bun run tests/test-sdk.ts",
    "build": "bun build src/index.ts --outdir dist --target node",
    "build:cli": "bun build src/cli/bin.ts --outdir dist/cli --target node",
    "build:binary": "bun build src/cli/bin.ts --compile --outfile isb",
    "build:all": "bun run build && bun run build:cli",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^20"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@stricli/core": "^1.2.5",
    "nanoid": "^5.1.6"
  },
  "keywords": ["incus", "lxc", "container", "vm", "sandbox", "typescript", "cli"],
  "license": "MIT"
}
